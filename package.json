    "font-awesome": "^4.7.0",
    "izitoast": "^1.4.0",
    "jquery": "^3.5.1",
    "lodash": "^4.17.20",
    "popper.js": "^1.16.1",
    "vue": "^2.6.12",
    "vue-head": "^2.2.0",
    "vue-lazyload": "^1.3.3",
    "vue-native-notification": "^1.1.1",
    "vue-router": "^3.4.7",
    "vuedraggable": "^2.24.3",
    "vuex": "^3.5.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.3.0",
