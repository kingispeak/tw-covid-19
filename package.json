{
  "name": "tw-covid-19",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --mode",
    "test:e2e": "npm run build && testcafe \"chrome:headless\" ./tests/e2e/*.test.js -S -s ./tests/e2e/screenshots --app \"http-server ./dist -p 3001 -s\"",
    "test": "testcafe chrome ./tests/e2e/*.test.js --app \"http-server ./dist -p 3001 -s\"",
    "lint": "vue-cli-service lint",
    "deploy": "npm run build production && gh-pages -d dist",
    "sandbox": "http-server ./dist -p 3001 -s",
    "format": "prettier --write './src/**/*.{js,vue}'"
  },
  "dependencies": {
    "animatewithsass": "^3.2",
    "axios": "^0.19.2",
    "bootstrap": "^4.5.2",
    "core-js": "^3.6.5",
    "dayjs": "^1.8.36",
    "echarts": "^4.9.0",
    "font-awesome": "^4.7.0",
    "izitoast": "^1.4.0",
    "jquery": "^3.5.1",
    "lodash": "^4.17.20",
    "popper.js": "^1.16.1",
    "vue": "^2.6.12",
    "vue-analytics": "^5.22.1",
    "vue-head": "^2.2.0",
    "vue-lazyload": "^1.3.3",
    "vue-native-notification": "^1.1.1",
    "vue-router": "^3.4.3",
    "vuedraggable": "^2.24.1",
    "vuex": "^3.5.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.3.0",
    "@vue/cli-plugin-eslint": "~4.3.0",
    "@vue/cli-plugin-router": "~4.3.0",
    "@vue/cli-plugin-vuex": "~4.3.0",
    "@vue/cli-service": "~4.3.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "gh-pages": "^2.2.0",
    "http-server": "^0.12.3",
    "prerender-spa-plugin": "^3.2.1",
    "prettier": "2.0.5",
    "sass": "^1.26.11",
    "sass-loader": "^8.0.2",
    "testcafe": "^1.9.3",
    "vue-cli-plugin-prerender-spa": "~1.1.6",
    "vue-template-compiler": "^2.6.12"
  },
  "homepage": "https://kingispeak.github.io/tw-covid-19"
}
