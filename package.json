{
  "name": "tw-covid-19",
  "version": "1.0.0",
  "homepage": "https://kingispeak.github.io/tw-covid-19",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:dev": "vue-cli-service build --mode development",
    "lint": "vue-cli-service lint",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "sandbox": "http-server ./dist -p 3001",
    "test:e2e": "testcafe \"chrome:headless\" ./tests/e2e/*.test.js -S -s ./tests/e2e/screenshots --app \"http-server ./dist -p 3001 -s\""
  },
  "dependencies": {
    "core-js": "^3.6.4",
    "echarts": "^4.7.0",
    "vue": "^2.6.11",
    "vue-router": "^3.1.6",
    "vuex": "^3.1.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.3.0",
    "@vue/cli-plugin-eslint": "~4.3.0",
    "@vue/cli-plugin-router": "~4.3.0",
    "@vue/cli-plugin-vuex": "~4.3.0",
    "@vue/cli-service": "~4.3.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "gh-pages": "^2.2.0",
    "http-server": "^0.12.1",
    "sass": "^1.26.3",
    "sass-loader": "^8.0.2",
    "testcafe": "^1.8.4",
    "vue-template-compiler": "^2.6.11"
  }
}
