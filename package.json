{
  "name": "tw-covid-19",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "format": "prettier --write './src/**/*.{js,vue}'",
    "test": "start-server-and-test serve 8080 'testcafe chrome ./tests/e2e/*.test.js'",
